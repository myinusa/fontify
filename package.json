{
  "name": "fontify",
  "displayName": "fontify",
  "description": "Change the UI font of VS Code",
  "version": "0.0.3",
  "publisher": "mythias",
  "engines": {
    "vscode": "^1.93.0"
  },
  "icon": "assets/images/hinting-at-vintage.png",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/myinusa/fontify"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fontify.enable",
        "title": "Fontify: Enable UI Font"
      },
      {
        "command": "fontify.disable",
        "title": "Fontify: Disable UI Font"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.11.1",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-security-node": "^1.1.4",
    "eslint-plugin-sonarjs": "^2.0.3",
    "eslint-plugin-unicorn": "^55.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
}
